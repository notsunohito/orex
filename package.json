{
  "name": "orex",
  "version": "0.0.7",
  "description": "Configureless state container.",
  "main": "lib/orex.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "test": "mocha --require babel-register test",
    "test:watch": "mocha --require babel-register --watch test",
    "build": "npm run flow:check && babel src --out-dir lib",
    "flow:check": "./node_modules/.bin/flow check"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/notsunohito/orex.git"
  },
  "bugs": {
    "url": "https://github.com/notsunohito/orex/issues"
  },
  "keywords": [
    "react",
    "flux",
    "redux",
    "state container"
  ],
  "author": "Shuichiro Kamiya",
  "license": "MIT",
  "engines": {
    "node": ">=6.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.8.0",
    "flow-bin": "^0.38.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {
    "deepmerge": "^1.3.2",
    "es2015-proxy": "^0.1.7",
    "react": "^15.4.2"
  }
}
